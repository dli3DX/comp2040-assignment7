   ],
   "source": [
    "import time\n",
    "\n",
    "for _ in range(5):\n",
    "    current_time = time.localtime()\n",
    "    minute = current_time.tm_min\n",
    "    if minute % 2 == 0:\n",
    "        print(f\"The minute ({minute}) is even.\")\n",
    "    else:\n",
    "        print(f\"The minute ({minute}) is odd.\")\n",
    "    time.sleep(60)\n"
   ]
